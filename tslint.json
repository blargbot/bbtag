{
    "defaultSeverity": "error",
    "extends": "tslint:latest",
    "jsRules": {},
    "rules": {
        "strict": true,
        "semicolon": true,
        "indent": [
            true,
            "spaces",
            4
        ],
        "quotemark": [
            true,
            "single"
        ],
        "no-undef": 1,
        "no-global-assign": 1,
        "comma-dangle": "warn",
        "no-trailing-spaces": "warn",
        "member-access": true,
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "property-declaration",
            "member-variable-declaration"
        ],
        "eofline": false,
        "max-classes-per-file": false,
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "variable-name": {
            "options": [
                "ban-keywords",
                "check-format",
                "allow-leading-underscore",
                "allow-pascal-case"
            ]
        },
        "object-literal-sort-keys": false,
        "unified-signatures": false,
        "no-unused-expression": false,
        "arrow-parens": false,
        "max-line-length": [
            true,
            180
        ]
    },
    "rulesDirectory": [
        "test"
    ]
}