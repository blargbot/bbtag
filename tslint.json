{
    "defaultSeverity": "error",
    "extends": "tslint:latest",
    "jsRules": {},
    "rules": {
        "semicolon": true,
        "member-access": true,
        "eofline": false,
        "max-classes-per-file": false,
        "object-literal-sort-keys": false,
        "unified-signatures": false,
        "no-unused-expression": false,
        "arrow-parens": false,
        "no-bitwise": false,
        "no-conditional-assignment": false,
        "no-empty": false,
        "no-this-assignment": false,
        "indent": [
            true,
            "spaces",
            4
        ],
        "quotemark": [
            true,
            "single"
        ],
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "property-declaration",
            "member-variable-declaration"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "variable-name": {
            "options": [
                "ban-keywords",
                "check-format",
                "allow-leading-underscore",
                "allow-pascal-case"
            ]
        },
        "max-line-length": [
            true,
            180
        ]
    }
}